          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnAutomaticMode" mnemonicParsing="false" onMouseClicked="#handleButtonClick" text="Automatic Mode" />
            <ComboBox fx:id="cbSelectElevator" prefWidth="150.0" GridPane.rowIndex="2" />
